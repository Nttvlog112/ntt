local Time = 1 -- Time in seconds -- thời gian đi tới , stop tp
local CFrameEnd =  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame -- tọa độ tp tới
local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd * CFrame.new(0,0,0) })
tween:Play()
end
end)

--tp island raid

game:GetService('RunService').RenderStepped:connect(function()
if _G.br2 then
                    local pos = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1")  -- bỏ cframe                  
                    local Distance = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                    if Distance < 150 then
                        Speed = 20000
                    elseif Distance < 200 then
                        Speed = 5000
                    elseif Distance < 300 then
                        Speed = 1030
                    elseif Distance < 500 then
                        Speed = 725
                    elseif Distance < 1000 then
                        Speed = 365
                    elseif Distance >= 1200 then
                        Speed = 330
                    end
                    local  CFend = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame -- tọa độ tp tới       
                    game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame = CFend * CFrame.new(0,45,0)}                 
                        ):Play()
                            
                   local pos = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2")  -- bỏ cframe                  
                    local Distance = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                    if Distance < 150 then
                        Speed = 20000
                    elseif Distance < 200 then
                        Speed = 5000
                    elseif Distance < 300 then
                        Speed = 1030
                    elseif Distance < 500 then
                        Speed = 725
                    elseif Distance < 1000 then
                        Speed = 365
                    elseif Distance >= 1200 then
                        Speed = 330
                    end
                    local  CFend = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame -- tọa độ tp tới       
                    game:GetService("TweenService"):Create(
                        game.Players.LocalPlayer.Character.HumanoidRootPart,
                        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
                        {CFrame = CFend * CFrame.new(0,45,0)}                 
                        ):Play()
                        
                  local pos = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3")  -- bỏ cframe                  
