bef5.Name = "bef5"
bef5.Parent = ef5
bef5.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
bef5.Position = UDim2.new(0.94000000, 0, 0.120000000, 0)
bef5.Size = UDim2.new(0, 20, 0, 20)
bef5.BorderColor3 = Color3.fromRGB(250, 250, 250)
bef5.Font = Enum.Font.SourceSans
bef5.Text = ""
bef5.TextColor3 = Color3.fromRGB(250, 250, 250)
