bef5.MouseButton1Down:connect(function()
--on off 
if bef5.Text == "" then --on
bef5.Text = "X"
_G.bef5 = true
game:GetService('RunService').RenderStepped:connect(function()
if _G.bef5 then --script
for _,v in pairs (workspace:GetChildren()) do
if v.Name == "Fruit" then

local pos = v.HumanoidRootPart --check pos
                    local Dis = (pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                  if Dis < 500 then
                  Speed = 725
                  elseif Dis < 200 then
                  Speed = 7000
                  elseif Dis < 150 then
                  Speed = 200000
                  elseif Dis < 1500 then
                  Speed = 110 
                  elseif Dis >= 2000 then
                        if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
            game.Players.LocalPlayer.Character.Humanoid.Health = 0
            end end              
                    if  game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    TP1(v.CFrame)
                    end                    
end end end
end)
elseif bef5.Text == "X" then --off
bef5.Text = ""
_G.bef5 = false
end
end)


