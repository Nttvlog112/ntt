bf3.Name = "bf3"
bf3.Parent = f3
bf3.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
bf3.Position = UDim2.new(0.94000000, 0, 0.120000000, 0)
bf3.Size = UDim2.new(0, 20, 0, 20)
